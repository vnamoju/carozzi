PROCEDURE "SCP_CAROZZI_SAC.Contabilidad_Coste::Z_DELE_COPC_MANUAL"(
		IN PERIODO_D VARCHAR(7), 
		IN PERIODO_H VARCHAR(7)
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
AS
BEGIN
	DELETE FROM "ZBW_0COPC_01"
	WHERE "ZBW_0COPC_01"."FISCPER" BETWEEN PERIODO_D AND PERIODO_H;
	DELETE FROM "ZBW_0COPC_02"
	WHERE "ZBW_0COPC_02"."FISCPER" BETWEEN PERIODO_D AND PERIODO_H;
END;
